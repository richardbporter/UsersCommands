{
    "name": "richardbporter/drush-users-commands",
    "description": "Drush commands to interact with multiple Drupal users.",
    "type": "drupal-drush",
    "keywords": ["drush", "drupal", "user-list", "list-users"],
    "authors": [
        {
            "name": "Richard B. Porter",
            "homepage": "http://www.richardbporter.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "composer/installers": "^1.4"
    },
    "require-dev": {
        "drush/drush": "~9",
        "phpunit/phpunit": "~5",
        "squizlabs/php_codesniffer": "~3"
    },
    "autoload-dev": {
        "psr-4": {
          "UsersCommands\\Tests\\": "tests/",
          "Unish\\": "vendor/drush/drush/tests/"
        }
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
